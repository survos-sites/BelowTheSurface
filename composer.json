{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "name": "survos-sites/bts",
    "description": "Netherlands project",
    "prefer-stable": true,
    "require": {
        "php": "^8.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "api-platform/doctrine-orm": "^4.2.12",
        "api-platform/symfony": "^4.2.12",
        "doctrine/doctrine-bundle": "^3.2.2",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6.1",
        "easycorp/easyadmin-bundle": "^4.27.6",
        "halaxa/json-machine": "^1.2.6",
        "inspector-apm/inspector-symfony": "^1.10",
        "league/csv": "^9.28.0",
        "liquid/liquid": "^1.4.44",
        "meilisearch/meilisearch-php": "dev-main",
        "museado/data-bundle": "^1.0",
        "nelmio/cors-bundle": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3.1",
        "survos/babel-bundle": "^2.0.88",
        "survos/ez-bundle": "^2.0.105",
        "survos/import-bundle": "^2.0.105",
        "survos/jsonl-bundle": "^2.0.105",
        "survos/lingua-bundle": "^2.0.85",
        "survos/lingua-contracts": "dev-main",
        "survos/lingua-core": "dev-main",
        "survos/media-bundle": "^2.0",
        "survos/meili-bundle": "^2.0.105",
        "survos/state-bundle": "^2.0.105",
        "survos/translator-bundle": "^2.0.88",
        "symfony/asset": "^7.4  || ^8.0",
        "symfony/asset-mapper": "^7.4  || ^8.0.3",
        "symfony/console": "^7.4  || ^8.0.3",
        "symfony/doctrine-messenger": "^7.4  || ^8.0.3",
        "symfony/dotenv": "^7.4  || ^8.0",
        "symfony/expression-language": "^7.4  || ^8.0",
        "symfony/flex": "^2.10",
        "symfony/form": "^7.4  || ^8.0.3",
        "symfony/framework-bundle": "^7.4  || ^8.0.3",
        "symfony/http-client": "^7.4  || ^8.0.3",
        "symfony/intl": "^7.4  || ^8.0.1",
        "symfony/mailer": "^7.4  || ^8.0.3",
        "symfony/mime": "^7.4  || ^8.0",
        "symfony/monolog-bundle": "^3.0|^4.0.1",
        "symfony/notifier": "^7.4  || ^8.0",
        "symfony/process": "^7.4  || ^8.0.3",
        "symfony/property-access": "^7.4  || ^8.0.3",
        "symfony/property-info": "^7.4  || ^8.0.3",
        "symfony/runtime": "^7.4  || ^8.0.1",
        "symfony/security-bundle": "^7.4  || ^8.0",
        "symfony/serializer": "^7.4  || ^8.0.3",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/string": "^7.4  || ^8.0.1",
        "symfony/translation": "^7.4  || ^8.0.3",
        "symfony/twig-bundle": "^7.4  || ^8.0.3",
        "symfony/ux-chartjs": "^2.32",
        "symfony/ux-icons": "^2.32",
        "symfony/ux-translator": "^2.32",
        "symfony/ux-turbo": "^2.32",
        "symfony/validator": "^7.4  || ^8.0.3",
        "symfony/web-link": "^7.4  || ^8.0",
        "symfony/yaml": "^7.4  || ^8.0.1",
        "twig/extra-bundle": "^3.22.2",
        "twig/intl-extra": "^3.22.1",
        "twig/twig": "^3.22.2|^4.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^8.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5.4",
        "roave/security-advisories": "dev-latest",
        "survos/code-bundle": "^2.0.105",
        "survos/deployment-bundle": "^2.0.105",
        "symfony/browser-kit": "^7.4  || ^8.0.3",
        "symfony/css-selector": "^7.4  || ^8.0",
        "symfony/debug-bundle": "^7.4  || ^8.0",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "^7.4  || ^8.0",
        "symfony/web-profiler-bundle": "^7.4  || ^8.0.3"
    }
}
