{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5",
        "league/csv": "^9.27",
        "liquid/liquid": "^1.4",
        "meilisearch/meilisearch-php": "dev-main",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.3",
        "survos/babel-bundle": "^2.0",
        "survos/ez-bundle": "^2.0",
        "survos/import-bundle": "^2.0",
        "survos/jsonl-bundle": "^2.0",
        "survos/lingua-bundle": "^2.0",
        "survos/meili-bundle": "^2.0",
        "survos/state-bundle": "^2.0",
        "symfony/asset": "^7.4  || ^8.0",
        "symfony/asset-mapper": "^7.4  || ^8.0",
        "symfony/console": "^7.4  || ^8.0",
        "symfony/doctrine-messenger": "^7.4  || ^8.0",
        "symfony/dotenv": "^7.4  || ^8.0",
        "symfony/expression-language": "^7.4  || ^8.0",
        "symfony/flex": "^2",
        "symfony/form": "^7.4  || ^8.0",
        "symfony/framework-bundle": "^7.4  || ^8.0",
        "symfony/http-client": "^7.4  || ^8.0",
        "symfony/intl": "^7.4  || ^8.0",
        "symfony/mailer": "^7.4  || ^8.0",
        "symfony/mime": "^7.4  || ^8.0",
        "symfony/monolog-bundle": "^3.0|^4.0",
        "symfony/notifier": "^7.4  || ^8.0",
        "symfony/process": "^7.4  || ^8.0",
        "symfony/property-access": "^7.4  || ^8.0",
        "symfony/property-info": "^7.4  || ^8.0",
        "symfony/runtime": "^7.4  || ^8.0",
        "symfony/security-bundle": "^7.4  || ^8.0",
        "symfony/serializer": "^7.4  || ^8.0",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "^7.4  || ^8.0",
        "symfony/translation": "^7.4  || ^8.0",
        "symfony/twig-bundle": "^7.4  || ^8.0",
        "symfony/ux-icons": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "^7.4  || ^8.0",
        "symfony/web-link": "^7.4  || ^8.0",
        "symfony/yaml": "^7.4  || ^8.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4  || ^8.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "survos/code-bundle": "^2.0",
        "survos/maker-bundle": "^1.6",
        "symfony/browser-kit": "^7.4  || ^8.0",
        "symfony/css-selector": "^7.4  || ^8.0",
        "symfony/debug-bundle": "^7.4  || ^8.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/stopwatch": "^7.4  || ^8.0",
        "symfony/web-profiler-bundle": "^7.4  || ^8.0"
    }
}
